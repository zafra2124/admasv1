{
  "expo": {
    "name": "Lottery Checker",
    "slug": "lottery-checker-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lotteryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-router",
        {
          "origin": "https://lottery-checker.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}